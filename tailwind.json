{
	"mb-2": {
		"style": {
			"marginBottom": 8
		}
	},
	"mb-4": {
		"style": {
			"marginBottom": 16
		}
	},
	"mb-6": {
		"style": {
			"marginBottom": 24
		}
	},
	"flex": {
		"style": {
			"display": "flex"
		}
	},
	"flex-1": {
		"style": {
			"flexGrow": 1,
			"flexShrink": 1,
			"flexBasis": "0%"
		}
	},
	"items-center": {
		"style": {
			"alignItems": "center"
		}
	},
	"justify-center": {
		"style": {
			"justifyContent": "center"
		}
	},
	"rounded-lg": {
		"style": {
			"borderTopLeftRadius": 8,
			"borderTopRightRadius": 8,
			"borderBottomRightRadius": 8,
			"borderBottomLeftRadius": 8
		}
	},
	"border": {
		"style": {
			"borderTopWidth": 1,
			"borderRightWidth": 1,
			"borderBottomWidth": 1,
			"borderLeftWidth": 1
		}
	},
	"border-gray-300": {
		"style": {
			"--tw-border-opacity": 1,
			"borderTopColor": "rgb(209 213 219 / var(--tw-border-opacity))",
			"borderRightColor": "rgb(209 213 219 / var(--tw-border-opacity))",
			"borderBottomColor": "rgb(209 213 219 / var(--tw-border-opacity))",
			"borderLeftColor": "rgb(209 213 219 / var(--tw-border-opacity))"
		}
	},
	"bg-blue-500": {
		"style": {
			"--tw-bg-opacity": 1,
			"backgroundColor": "rgb(59 130 246 / var(--tw-bg-opacity))"
		}
	},
	"bg-white": {
		"style": {
			"--tw-bg-opacity": 1,
			"backgroundColor": "rgb(255 255 255 / var(--tw-bg-opacity))"
		}
	},
	"p-2": {
		"style": {
			"paddingTop": 8,
			"paddingRight": 8,
			"paddingBottom": 8,
			"paddingLeft": 8
		}
	},
	"p-3": {
		"style": {
			"paddingTop": 12,
			"paddingRight": 12,
			"paddingBottom": 12,
			"paddingLeft": 12
		}
	},
	"px-4": {
		"style": {
			"paddingLeft": 16,
			"paddingRight": 16
		}
	},
	"text-center": {
		"style": {
			"textAlign": "center"
		}
	},
	"text-lg": {
		"style": {
			"fontSize": 18,
			"lineHeight": 28
		}
	},
	"text-sm": {
		"style": {
			"fontSize": 14,
			"lineHeight": 20
		}
	},
	"text-xl": {
		"style": {
			"fontSize": 20,
			"lineHeight": 28
		}
	},
	"font-bold": {
		"style": {
			"fontWeight": "700"
		}
	},
	"text-white": {
		"style": {
			"--tw-text-opacity": 1,
			"color": "rgb(255 255 255 / var(--tw-text-opacity))"
		}
	}
}